{
  "version": 2,
  "builds": [
    {
      "src": "frontend/src/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|json|css|png|jpg|jpeg|gif|ico|woff|woff2|ttf|svg))",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/.*",
      "dest": "frontend/src/index.html"
    }
  ]
}
